# Frontend environment variables for server deployment

# Use same-origin paths so Nginx can proxy /api and /admin/api to backend
VITE_API_URL=/api
VITE_ADMIN_API_URL=/admin/api
VITE_ADMIN_API_BASE_URL=/admin/api
VITE_ENV=production

# Feature gate is disabled by default; set to true only if today-feature schedule is used.
VITE_GATE_TODAY_FEATURE=false

# Disable demo/debug for production
VITE_ENABLE_DEMO_FALLBACK=false
VITE_ENABLE_DEBUG=false
